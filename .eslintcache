/Coding-Development/projects/react-projects/react-spring-todo/src/components/AuthenticatedRoute.js",[],"/Users/bal/Development/Coding-Development/projects/react-projects/react-spring-todo/src/components/Welcome.js",[],"/Users/bal/Development/Coding-Development/projects/react-projects/react-spring-todo/src/api/user/UserService.js",[],"/Users/bal/Development/Coding-Development/projects/react-projects/react-spring-todo/src/components/todos/TodoList.js",[],"/Users/bal/Development/Coding-Development/projects/react-projects/react-spring-todo/src/api/todo/TodoService.js",[],"/Users/bal/Development/Coding-Development/projects/react-projects/react-spring-todo/src/components/todos/Todo.js",[],"/Users/bal/Development/Coding-Development/projects/react-projects/react-spring-todo/src/components/todos/TodoForm.js",["86","87"],"import React, {useEffect, useState } from 'react';\nimport { Button, Form, Modal, Row } from 'react-bootstrap';\nimport { useForm } from 'react-hook-form';\nimport moment from 'moment';\nimport styled from 'styled-components';\nimport { getTodo, createTodo, updateTodo } from '../../api/todo/TodoService';\nimport { getLoggedInUser } from '../../auth/AuthenticationService';\nimport FormButton from '../form-components/FormButton';\n\nconst StyledRow = styled(Row)`\n  justify-content: center;\n  margin-top: 13rem;\n\n  & form {\n    width: 325px;\n  }\n  & .form-label { font-weight: 500; }\n  & p {\n    color: red; \n    font-size: 0.8rem;\n  }\n`\n\nconst TodoForm = props => {\n  const { register, handleSubmit, errors } = useForm()\n  const [todo, setTodo] = useState({\n    id: null, \n    description: '',\n    targetDate: ''\n  })\n\n  useEffect(() => {\n    if (props.match.url !== '/todos/new') {\n      getTodo(props.match.params.id, getLoggedInUser())\n      .then(({ data }) => setTodo({\n        id: data.id, \n        description: data.description, \n        targetDate: moment.utc(data.targetDate).format('YYYY-MM-DD'),\n        completed: data.completed\n      }))\n    }\n  }, [props.match.params.id, props.match.url])\n\n  const onInputChange = event => {\n    const { name, value } = event.target\n    setTodo({ ...todo, [name]: value })\n  }\n\n  const onSubmit = data => {\n    const username = getLoggedInUser()\n    if (todo.id === null) {\n      createTodo(username, { ...todo })\n      .then(() => props.history.push('/todos'))\n    } else {\n      updateTodo(props.match.params.id, username, todo)\n      .then(() => props.history.push('/todos'))\n    }\n  }\n\n  return (\n    <StyledRow>\n      <Form onSubmit={handleSubmit(onSubmit)}>\n        <Form.Group>\n          <Form.Label>Todo</Form.Label>\n          <Form.Control \n            type=\"text\"\n            name=\"description\"\n            value={todo.description}\n            onChange={onInputChange}\n            ref={register({ required: \"Description required\" })}\n            />\n          {errors.description && <p>{errors.description.message}</p>}\n        </Form.Group>\n        <Form.Group>\n          <Form.Label>Date to be Completed</Form.Label>\n          <Form.Control \n            type=\"date\"\n            name=\"targetDate\"\n            value={todo.targetDate}\n            onChange={onInputChange}\n            ref={register({ required: \"Date required\" })}\n          />\n          {errors.targetDate && <p>{errors.targetDate.message}</p>}\n        </Form.Group>\n        <FormButton text={\"Save\"} />\n      </Form>\n    </StyledRow>\n  )\n}\n\nexport default TodoForm;\n","/Users/bal/Development/Coding-Development/projects/react-projects/react-spring-todo/src/components/todos/DeleteModal.js",[],"/Users/bal/Development/Coding-Development/projects/react-projects/react-spring-todo/src/components/NotFoundError.js",[],{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","severity":1,"message":"93","line":2,"column":10,"nodeType":"94","messageId":"95","endLine":2,"endColumn":16},{"ruleId":"92","severity":1,"message":"96","lin